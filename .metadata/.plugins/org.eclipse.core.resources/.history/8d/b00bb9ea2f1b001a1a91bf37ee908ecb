package chapter.one.introduction.to.java.programming;

public class Egg {
	public static void main(String[] args) {
		System.out.print(draw());
	}
	
	public static String draw() {
		String firstLine = "_______\n";
		String secondLine = "/       \\\n";
		String thirdLine = "";
		String fourthLine = "";
		String fifthLine = "";
		String sixthLine = "";
		String seventhLine = "";
		String eightLine = "";
		String ninthLine = "";
		return firstLine + secondLine + thirdLine + fourthLine + fifthLine + sixthLine + seventhLine + eightLine + ninthLine;
	}

}
