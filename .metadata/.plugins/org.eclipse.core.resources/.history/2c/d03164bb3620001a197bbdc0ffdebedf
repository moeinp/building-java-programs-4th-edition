package one;

//Write a complete Java program in a class named Egg that displays the following output:
//
//	  _______
//	 /       \
//	/         \
//	-"-'-"-'-"-
//	\         /
//	 \_______/

public class Egg {
	public static void main(String[] args) {
		System.out.print(draw());
	}

	public static String draw() {
		String firstLine = "  _______\n";
		String secondLine = " /       \\\n";
		String thirdLine = "/         \\\n";
		String fourthLine = "-\"-'-\"-'-\"-\n";
		String fifthLine = "\\         /\n";
		String sixthLine = " \\_______/";
		return firstLine + secondLine + thirdLine + fourthLine + fifthLine + sixthLine;
	}

}
